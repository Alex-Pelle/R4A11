<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="311dp"
        android:layout_height="151dp"
        
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        >


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="151dp"
            android:orientation="vertical"
            tools:ignore="UselessParent">

            <TextView
                android:id="@+id/jourDeLaSemainePlanningVisualizer"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginTop="10dp"
                android:text="Jour de la semaine"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/titreDeLaSeancePlanningVisualizer"
                android:layout_width="match_parent"
                android:layout_height="61dp"
                android:text="Titre de la sÃ©ance"
                android:textAlignment="center" />

            <TextView
                android:id="@+id/nombreExercicesPlanningVisualizer"
                android:layout_width="match_parent"
                android:layout_height="33dp"
                android:text="Nombre d'exercices :"
                android:textAlignment="center" />
        </LinearLayout>
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>